{
  "name": "@secondlayer/cli",
  "version": "0.2.2",
  "description": "CLI for generating type-safe contract interfaces for the Stacks blockchain",
  "type": "module",
  "bin": {
    "secondlayer": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "import": "./dist/plugins/index.js"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "stacks",
    "clarity",
    "blockchain",
    "smart-contracts",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@antfu/ni": "^24.4.0",
    "@hirosystems/clarinet-sdk": "^3.0.2",
    "@secondlayer/clarity-types": "^0.2.0",
    "@stacks/connect": "^8.1.9",
    "@stacks/transactions": "7.0.6",
    "@tanstack/react-query": "^5.77.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "esbuild": "^0.19.0",
    "execa": "^9.5.3",
    "fast-glob": "^3.3.3",
    "got": "^13.0.0",
    "ora": "^7.0.1",
    "prettier": "^3.1.0",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "@types/react": "^19.1.5"
  },
  "engines": {
    "node": ">=18"
  }
}
