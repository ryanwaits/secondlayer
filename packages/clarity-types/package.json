{
  "name": "@secondlayer/clarity-types",
  "version": "0.4.1",
  "description": "TypeScript type definitions and utilities for Clarity smart contract ABIs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "stacks",
    "clarity",
    "blockchain",
    "smart-contracts",
    "typescript",
    "types"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@stacks/transactions": "^7.0.0"
  },
  "devDependencies": {
    "@vitest/ui": "^1.2.1"
  },
  "engines": {
    "node": ">=18"
  }
}
